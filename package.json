{
  "name": "mi-portafolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run prebuild:prod && ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prebuild:prod": "node dotenv-loader.js",
    "build:prod": "npm run prebuild:prod && ng build --configuration production --base-href /MyPortfolio/",
    "deploy": "npm run build:prod && npx angular-cli-ghpages --dir=dist --repo=https://github.com/loplop8/MyPortfolio.git --branch=fix --no-silent"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "bootstrap": "^5.3.0",
    "dotenv": "^16.5.0",
    "install": "^0.13.0",
    "npm": "^11.1.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.17.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.0",
    "@types/bootstrap": "^5.2.10",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}
